<p style="color: green"><%= notice %></p>

<h1>Maps</h1>

<div id="map" style="height: 600px; width: 700px;"></div>

<div id="maps">
  <% @maps.each do |map| %>
    <%= render map %>
    <p>
      <%= link_to "Show this map", map %>
    </p>
  <% end %>
</div>

<%= link_to "New map", new_map_path %>
